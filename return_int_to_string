#include <iostream>
#include <cstdio>
#include <string> 

#include <assert.h>

using namespace std;

int* result(int& fnum) {

	int c = 3;
	int arr[8]{};
	int i; 
	for (i = 0; fnum != 1; i++)
	{
		c = fnum % 2;
		fnum = fnum / 2;
		arr[i] = c;
		cout << arr[i];

	}
	arr[i] = 1;
	cout << arr[i] << endl;



	return arr;

}



int main()
{
	int fnum;
	cout << "enter num";
	cin >> fnum;
	result(fnum);

}

