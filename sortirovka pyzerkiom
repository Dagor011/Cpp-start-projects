#include <iostream>
#include <time.h>
using namespace std;

int main()
{
    int* arr; 
    int size; 

    cout << "enter num";
    cin >> size;


    arr = new int[size]; 

    for (int i = 0; i < size; i++) {
        arr[i] = rand() % 10;
        cout << arr[i];
    }

    int temp; 

    for (int i = 0; i < size - 1; i++) {
        for (int j = 0; j < size - i - 1; j++) {
            if (arr[j] > arr[j + 1]) {
                
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;
            }
        }
    }

    
    for (int i = 0; i < size; i++) {
        cout << arr[i] << " ";
    }
    cout << endl;

    delete[] arr;

    return 0;
}
